                "page",
                "resources_per_page",
                "total_resources"
            ],
            "properties": {
                "page": {
                    "title": "Page",
                    "type": "integer",
                    "examples": [
                        1
                    ],
                    "default": 0
                },
                "resources_per_page": {
                    "title": "Resources_per_page",
                    "type": "integer",
                    "examples": [
                        10
                    ],
                    "default": 0
                },
                "total_resources": {
                    "title": "Total_resources",
                    "type": "integer",
                    "examples": [
                        11
                    ],
                    "default": 0
                }
            }
        },
        "data": {
            "title": "Data",
            "type": "array",
            "default": [],
            "items": {
                "title": "Items",
                "type": "object",
                "required": [
                    "type",
                    "id",
                    "attributes",
                    "relationships",
                    "links"
                ],
                "properties": {
                    "type": {
                        "title": "Type",
                        "type": "string",
                        "default": "",
