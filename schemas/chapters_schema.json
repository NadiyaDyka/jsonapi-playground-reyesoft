                    "attributes": {
                        "title": "Attributes",
                        "type": "object",
                        "required": [
                            "title",
                            "ordering"
                        ],
                        "properties": {
                            "title": {
                                "title": "Title",
                                "type": "string",
                                "default": "",
                                "examples": [
                                    "Chapter 100"
                                ]
                            },
                            "ordering": {
                                "title": "Ordering",
                                "type": "integer",
                                "examples": [
                                    7372164
                                ],
                                "default": 0
                            }
                        }
                    },
                    "relationships": {
                        "title": "Relationships",
                        "type": "object",
                        "required": [
                            "photos",
                            "book"
                        ],
                        "properties": {
                            "photos": {
                                "title": "Photos",
                                "type": "object",
                                "required": [
                                    "data"
                                ],
                                "properties": {
                                    "data": {
                                        "title": "Data",
                                        "type": "array",
                                        "default": [],
                                        "items": {
                                            "title": "Items",
                                            "type": "object",
                                            "required": [
                                                "type",
                                                "id"
                                            ],
                                            "properties": {
                                                "type": {
                                                    "title": "Type",
                                                    "type": "string",
                                                    "default": "",
