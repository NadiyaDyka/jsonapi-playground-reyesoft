                                                        "photos"
                                                    ]
                                                },
                                                "id": {
                                                    "title": "Id",
                                                    "type": "string",
                                                    "default": "",
                                                    "examples": [
                                                        "201"
                                                    ]
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            "book": {
                                "title": "Book",
                                "type": "object",
                                "required": [
                                    "data"
                                ],
                                "properties": {
                                    "data": {
                                        "title": "Data",
                                        "type": "object",
                                        "required": [
                                            "type",
                                            "id"
                                        ],
                                        "properties": {
