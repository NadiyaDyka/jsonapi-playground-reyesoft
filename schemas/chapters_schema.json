                                            },
                                            "id": {
                                                "title": "Id",
                                                "type": "string",
                                                "default": "",
                                                "examples": [
                                                    "43"
                                                ]
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "links": {
                        "title": "Links",
                        "type": "object",
                        "required": [
                            "self"
                        ],
                        "properties": {
                            "self": {
                                "title": "Self",
                                "type": "string",
                                "default": "",
                                "examples": [
                                    "/chapters/1"
                                ]
                            }
                        }
                    }
                }
            }
        }
