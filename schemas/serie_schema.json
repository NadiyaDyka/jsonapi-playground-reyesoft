{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "meta",
        "data"
    ],
    "properties": {
        "meta": {
            "type": "array",
            "default": []
        },
        "data": {
            "type": "object",
            "required": [
                "type",
                "id",
                "attributes",
                "relationships",
                "links"
            ],
            "properties": {
                "type": {
                    "type": "string",
                    "default": "",
                    "examples": [
                        "series"
                    ],
                    "pattern": "^.*$"
                },
                "id": {
                    "type": "string",
                    "default": "",
                    "examples": [
                        "2"
                    ],
                    "pattern": "^.*$"
                },
                "attributes": {
                    "type": "object",
                    "required": [
                        "title"
                    ],
                    "properties": {
                        "title": {
                            "type": "string",
                            "default": "",
                            "examples": [
                                "Series 1348819"
                            ],
                            "pattern": "^.*$"
                        }
                    }
                },
                "relationships": {
                    "type": "object",
                    "required": [
                        "photos",
                        "books"
                    ],
                    "properties": {
                        "photos": {
                            "type": "object",
                            "required": [
                                "data"
                            ],
                            "properties": {
                                "data": {
                                    "type": "array",
                                    "default": []
                                }
                            }
                        },
                        "books": {
                            "type": "object",
                            "required": [
                                "data"
                            ],
                            "properties": {
                                "data": {
                                    "type": "array",
                                    "default": []
                                }
                            }
                        }
                    }
                },
                "links": {
                    "type": "object",
                    "required": [
                        "self"
                    ],
                    "properties": {
                        "self": {
                            "type": "string",
                            "default": "",
                            "examples": [
                                "/series/2"
                            ],
                            "pattern": "^.*$"
                        }
                    }
                }
            }
        }
    }
}